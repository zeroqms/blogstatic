{
  "headers": [
    {
      "source": "/config.json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=300"
        }
      ]
    },
    {
      "source": "/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/post/*",
      "destination": "/index.html"
    }
  ]
}